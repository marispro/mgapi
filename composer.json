{
    "name": "tde/mgapi",
    "description": "Mailigen.com PHP API wrapper",
    "keywords": [
        "mgapi",
        "Mailigen",
        "mailing"
    ],
    "homepage": "https://github.com/tdeNL/mgapi",
    "license": "MIT",
    "authors": [
        {
            "name": "Jerome Anker",
            "email": "jerome@tde.nl",
            "homepage": "https://www.tde.nl",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.0",
        "ext-curl": "^7.1",
        "ext-json": "^1.5"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.4"
    },
    "autoload": {
        "psr-4": {
            "Mailigen\\MGAPI\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Mailigen\\MGAPI\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    }
}
